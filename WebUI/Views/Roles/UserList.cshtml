@model IEnumerable<WebUI.Models.User>

<h2>Список пользователей</h2>
<a class="nav-link text-dark" asp-area="" asp-controller="Roles" asp-action="Index">Назад</a>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.UserName</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Права доступа</a>
            </td>
        </tr>
    }
</table>